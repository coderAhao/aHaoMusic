.rankingList {
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
}
.left-img {
  width: 120rpx;
  height: 120rpx;
  display: block;
  margin-right: 15rpx;
}
.left-text {
  flex: 1;
}
.left-text .title {
  font-size: 35rpx;
}
.left-text .content {
  font-size: 30rpx;
  color: #999;
}
.right .img {
  width: 50rpx;
  height: 50rpx;
}
